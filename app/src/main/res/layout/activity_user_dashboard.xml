        android:layout_width="360dp"
        android:layout_height="730dp"
        android:background="#2F2F2F"
        android:backgroundTint="@color/whites"
        android:orientation="vertical"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableLayout
            android:id="@+id/searchDevices"
            android:layout_width="match_parent"
            android:layout_height="100dp">

            <TableRow
                android:layout_width="350dp"
                android:layout_height="match_parent"
                android:paddingLeft="25dp">

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="305dp"
                    android:layout_height="62dp"
                    app:boxBackgroundColor="@color/black"
                    app:boxBackgroundMode="outline"
                    app:boxCornerRadiusBottomEnd="30dp"
                    app:boxCornerRadiusBottomStart="30dp"
                    app:boxCornerRadiusTopEnd="30dp"
                    app:boxCornerRadiusTopStart="30dp"
                    app:boxStrokeColor="@color/black"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.531"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.107"
                    app:placeholderText="Search devices"
                    app:placeholderTextColor="@color/whites"
                    app:startIconDrawable="@drawable/baseline_search_24"
                    app:startIconTint="@color/whites">

                    <com.google.android.material.textfield.TextInputEditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:backgroundTint="#2F2F2F"
                        android:fontFamily="@font/roboto_bold"
                        android:textColor="@color/whites"
                        android:textSize="14sp" />

                </com.google.android.material.textfield.TextInputLayout>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

        </TableLayout>

        <TableLayout
            android:id="@+id/imSearchingFor"
            android:layout_width="match_parent"
            android:layout_height="150dp">

            <TableRow
                android:layout_width="350dp"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/tvImSearchingFor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_bold"
                    android:paddingLeft="10dp"
                    android:text="Im searching for"
                    android:textColor="#2F2F2F"
                    android:textSize="20sp" />
            </TableRow>

            <TableRow
                android:layout_width="350dp"
                android:layout_height="match_parent"
                android:paddingLeft="50dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/button17"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_weight="1"
                        android:backgroundTint="#2F2F2F"
                        android:fontFamily="@font/roboto_bold"
                        android:text="Apple"
                        android:textColor="@color/whites" />

                    <Button
                        android:id="@+id/button16"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_weight="1"
                        android:backgroundTint="#2F2F2F"
                        android:fontFamily="@font/roboto_bold"
                        android:text="Samsung"
                        android:textColor="@color/whites" />

                    <Button
                        android:id="@+id/button15"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:backgroundTint="#2F2F2F"
                        android:fontFamily="@font/roboto_bold"
                        android:text="Xiaomi"
                        android:textColor="@color/whites" />
                </LinearLayout>

            </TableRow>

            <TableRow
                android:layout_width="350dp"
                android:layout_height="match_parent"
                android:paddingLeft="80dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/button20"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:backgroundTint="#2F2F2F"
                        android:fontFamily="@font/roboto_bold"
                        android:text="Realme"
                        android:textColor="@color/whites"
                        tools:layout_editor_absoluteX="-44dp"
                        tools:layout_editor_absoluteY="275dp" />

                    <Button
                        android:id="@+id/button27"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:backgroundTint="#2F2F2F"
                        android:fontFamily="@font/roboto_bold"
                        android:text="Latest"
                        android:textColor="@color/whites"
                        tools:layout_editor_absoluteX="-44dp"
                        tools:layout_editor_absoluteY="307dp" />
                </LinearLayout>

            </TableRow>

        </TableLayout>

        <TableLayout
            android:id="@+id/Type"
            android:layout_width="match_parent"
            android:layout_height="100dp">

            <TableRow
                android:layout_width="350dp"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/tvType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_bold"
                    android:paddingLeft="10dp"
                    android:text="Type"
                    android:textColor="#2F2F2F"
                    android:textSize="20sp" />
            </TableRow>

            <TableRow
                android:layout_width="350dp"
                android:layout_height="match_parent"
                android:paddingLeft="50dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/SmartphoneButton"
                        android:layout_width="130dp"
                        android:layout_height="40dp"
                        android:backgroundTint="#2F2F2F"
                        android:fontFamily="@font/roboto_bold"
                        android:text="Smartphone"
                        android:textColor="@color/whites" />

                    <Button
                        android:id="@+id/TabletButton"
                        android:layout_width="130dp"
                        android:layout_height="40dp"
                        android:backgroundTint="#2F2F2F"
                        android:fontFamily="@font/roboto_bold"
                        android:text="Tablet"
                        android:textColor="@color/whites" />
                </LinearLayout>

            </TableRow>

        </TableLayout>

        <TableLayout
            android:id="@+id/Rating"
            android:layout_width="match_parent"
            android:layout_height="150dp">

            <TableRow
                android:layout_width="350dp"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/tvRating"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_bold"
                    android:padding="10dp"
                    android:text="Rating"
                    android:textColor="#2F2F2F"
                    android:textSize="20sp" />
            </TableRow>

            <TableRow
                android:layout_width="350dp"
                android:layout_height="match_parent"
                android:paddingLeft="40dp">

                <Button
                    android:id="@+id/button6"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:backgroundTint="#2F2F2F"
                    android:fontFamily="@font/roboto_bold"
                    android:text="1 Tally"
                    android:textColor="@color/whites" />

                <Button
                    android:id="@+id/button7"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:backgroundTint="#2F2F2F"
                    android:fontFamily="@font/roboto_bold"
                    android:text="2 Tally"
                    android:textColor="@color/whites"
                    android:textColorHighlight="@color/whites" />

                <Button
                    android:id="@+id/button8"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:backgroundTint="#2F2F2F"
                    android:fontFamily="@font/roboto_bold"
                    android:insetRight="0dp"
                    android:text="3 Tally"
                    android:textColor="@color/whites"
                    app:iconPadding="0dp" />
            </TableRow>

            <TableRow
                android:layout_width="350dp"
                android:layout_height="match_parent"
                android:paddingLeft="80dp">

                <Button
                    android:id="@+id/button10"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:backgroundTint="#2F2F2F"
                    android:fontFamily="@font/roboto_bold"
                    android:text="4 Tally"
                    android:textColor="@color/whites" />

                <Button
                    android:id="@+id/button11"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:backgroundTint="#2F2F2F"
                    android:fontFamily="@font/roboto_bold"
                    android:text="5 Tally"
                    android:textColor="@color/whites" />
            </TableRow>

        </TableLayout>

        <TableLayout
            android:id="@+id/PriceRange"
            android:layout_width="349dp"
            android:layout_height="179dp">

            <TableRow
                android:id="@+id/RowTextPriceRange"
                android:layout_width="350dp"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/tvPriceRange"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_bold"
                    android:paddingLeft="10dp"
                    android:text="Price range"
                    android:textColor="#2F2F2F"
                    android:textSize="20sp" />
            </TableRow>

            <TableRow
                android:id="@+id/RowPriceRange"
                android:layout_width="350dp"
                android:layout_height="127dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:paddingLeft="30dp">

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:backgroundTint="#2F2F2F"
                        android:textColorHint="#2F2F2F"
                        app:hintTextColor="#2F2F2F"
                        tools:layout_editor_absoluteX="-70dp"
